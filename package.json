{
  "name": "pdf2htmlex-nodejs-server",
  "version": "1.0.0",
  "description": "A nodejs server for utilizing pdf2htmlEX to transcode pdf to html.",
  "main": "dist/index.js",
  "scripts": {
    "tsc": "tsc",
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "build": "npm-run-all clean tsc copy-assets",
    "db_init:dev": "cd src/database && sequelize db:migrate && sequelize db:seed:all",
    "db_init": "export NODE_ENV=production && cd dist/database && sequelize db:migrate && sequelize db:seed:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --watch src -e ts,ejs --exec ts-node -r dotenv/config src/index.ts dotenv_config_path=.env.dev",
    "debug": "nodemon --verbose --watch src -e ts --exec node -r dotenv/config --inspect-brk -r ts-node/register src/index.ts dotenv_config_path=.env.dev",
    "prestart": "npm run build",
    "start": "node -r dotenv/config . dotenv_config_path=.env"
  },
  "repository": {
    "type": "git",
    "url": "https://192.168.0.33/git/LiaoHongXing/pdf2htmlEX.git"
  },
  "keywords": [
    "pdf2htmlEX",
    "nodejs",
    "server",
    "dockerize"
  ],
  "author": "Hongxing Liao",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "archiver": "^4.0.2",
    "bcrypt": "^5.0.0",
    "bull": "^3.15.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "helmet": "^3.23.3",
    "http-errors": "^1.8.0",
    "joi": "^17.1.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "node-cron": "^2.0.3",
    "sequelize": "^6.3.0",
    "shelljs": "^0.8.4",
    "sqlite3": "^5.0.0",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@types/archiver": "^3.1.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bull": "^3.14.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/express-jwt": "0.0.42",
    "@types/helmet": "0.0.47",
    "@types/http-errors": "^1.6.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/multer": "^1.4.3",
    "@types/node-cron": "^2.0.3",
    "@types/shelljs": "^0.8.8",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^29.1.3",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,html}": [
      "eslint --fix"
    ]
  }
}
