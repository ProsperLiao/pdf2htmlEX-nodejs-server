<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('partials/head', { page }) %>
</head>
<body>
<%- include('partials/header', { page }) %>
<div class="bg-3 position-absolute" style="top:60px; bottom: 0; left: 15px; right: 15px;">
        <% if (page === 'error') { %>
        <%- include('partials/error', { errno, url, errmsg }) %>
        <% } else if (page === 'users') { %>
            <%- include('partials/user') %>
        <% } else if (page === 'doc') { %>
            <%- include('partials/doc') %>
        <% } else if (page === 'login') { %>
            <%- include('partials/login') %>
        <% } else { %>
            <%- include('partials/home') %>
        <% } %>
</div>
</body>
<%- include('partials/script') %>
</html>
